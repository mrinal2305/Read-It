{
  "name": "read-it",
  "version": "1.0.0",
  "description": "A minimal Electron application to save your bookmark",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "mac"  :"electron-builder -m",
    "win"  :"electron-builder -w",
    "build"  :"electron-builder -mwl"
  },
  "build" : {
    "appId" : "com.mkk.Read It",
    "directories" : {
      "buildResources" : "."
    }
  },
  "mac" : {
    "category" : "public.app-category.productivity",
    "target"   : "dmg"
  },
  "win" : {

  },
  "linux" : {
    "category" : "network"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Mrinal Kumar Karn",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^15.5.5",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "electron-reload": "^1.5.0",
    "electron-window-state": "^5.0.3",
    "nodemon": "^1.19.3",
    "watch": "^1.0.2"
  }
}
